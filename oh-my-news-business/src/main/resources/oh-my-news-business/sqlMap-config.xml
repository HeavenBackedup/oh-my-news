<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE sqlMapConfig PUBLIC "-//ibatis.apache.org//DTD SQL Map Config 2.0//EN"
        "http://ibatis.apache.org/dtd/sql-map-config-2.dtd">
<sqlMapConfig>
    <settings cacheModelsEnabled="true"
              enhancementEnabled="true"
              lazyLoadingEnabled="false"
              errorTracingEnabled="true"
              maxTransactions="100"
              maxRequests="320"
              maxSessions="100"
              useStatementNamespaces="true"/>
    <sqlMap resource="ibatis/template_sqlMap.xml"></sqlMap>
    <sqlMap resource="ibatis/media_sqlMap.xml"></sqlMap>
    <sqlMap resource="ibatis/category_sqlMap.xml"></sqlMap>
    <sqlMap resource="ibatis/concern_sqlMap.xml"></sqlMap>
    <sqlMap resource="ibatis/a_m_rel_sqlMap.xml"></sqlMap>
    <sqlMap resource="ibatis/article_sqlMap.xml"></sqlMap>
    <sqlMap resource="ibatis/comment_sqlMap.xml"></sqlMap>
    <sqlMap resource="ibatis/private_message_sqlMap.xml"></sqlMap>
    <sqlMap resource="ibatis/transaction_sqlMap.xml"></sqlMap>
    <sqlMap resource="ibatis/u_a_rel_sqlMap.xml"></sqlMap>
    <sqlMap resource="ibatis/user_sqlMap.xml"></sqlMap>
    <sqlMap resource="ibatis/wallet_sqlMap.xml"></sqlMap>

</sqlMapConfig>